  <ul id="results"></ul>
  <script>
const results = document.querySelector("#results");

fetch("https://api.punkapi.com/v2/beers?brewed_after=12-2015&abv_gt=5&abv_lt=11")
  .then(response => response.json())
  .then((data) => {
    data.Search.forEach((result) => {
      const movie = `<li>
        <img src="${result.Poster}" alt="">
        <p>${result.Title}</p>
      </li>`;
      results.insertAdjacentHTML("beforeend", movie);
    });
  });
  </script>

  <header class="intro"></header>
  <section id="html" class="container"><h2><i class="fab fa-html5"></i></h2></section>
  <section id="css" class="container"><h2><i class="fab fa-css3-alt"></i></h2></section>
